<OpenIG-Config>
  <ShadowedScene-minLightMargin>10.0</ShadowedScene-minLightMargin>
  <ShadowedScene-maxFarPlane>500.0</ShadowedScene-maxFarPlane>
  <ShadowedScene-texSize>4096</ShadowedScene-texSize>
  <ShadowedScene-baseTexUnit>0</ShadowedScene-baseTexUnit>
  <ShadowedScene-shadowTexUnit>1</ShadowedScene-shadowTexUnit>
  <Default-diffuse-texture-slot>0</Default-diffuse-texture-slot>
  <Clouds-Shadows-Texture-Slot>6</Clouds-Shadows-Texture-Slot>
  <Enable-Clouds-Shadows>yes</Enable-Clouds-Shadows>
  <Shadows-Factor>0.3</Shadows-Factor>
  <Lighting-Implementation-Texture-Slot>5</Lighting-Implementation-Texture-Slot>
  <SplashScreen></SplashScreen>
  <Shadowed-GPU-Vegetation>no</Shadowed-GPU-Vegetation>
  <ForwardPlusLightsLODCulling>yes</ForwardPlusLightsLODCulling>
  <ForwardPlusLightsDefaultLODRange>500</ForwardPlusLightsDefaultLODRange>
  <LogZDepthBuffer>yes</LogZDepthBuffer>
  <GroupLightsBasedOnXMLDefinition>yes</GroupLightsBasedOnXMLDefinition>
  <ImageGenerator-Plugins-Config>
    <Plugin>
      <Order-Number>-3</Order-Number>
      <Name>SilverLining</Name>
    </Plugin>
    <Plugin>
      <Order-Number>-2</Order-Number>
      <Name>ModelComposition</Name>
    </Plugin>
    <Plugin>
      <Order-Number>-1</Order-Number>
      <Name>ForwardPlusLighting</Name>
    </Plugin>
    <Plugin>
      <Order-Number>2</Order-Number>
      <Name>VDBOffset</Name>
    </Plugin>
    <Plugin>
      <Order-Number>3</Order-Number>
      <Name>Animation</Name>
    </Plugin>
    <Plugin>
      <Order-Number>6</Order-Number>
      <Name>GPUVegetation</Name>
    </Plugin>
    <Plugin>
      <Order-Number>8</Order-Number>
      <Name>FBXAnimation</Name>
    </Plugin>
    <Plugin>
      <Order-Number>9</Order-Number>
      <Name>OSGParticleEffects</Name>
    </Plugin>
    <Plugin>
      <Order-Number>10</Order-Number>
      <Name>LightsControl</Name>
    </Plugin>
    <Plugin>
      <Order-Number>11</Order-Number>
      <Name>UI</Name>
    </Plugin>
    <Plugin>
      <Order-Number>12</Order-Number>
      <Name>Bullet</Name>
    </Plugin>
  </ImageGenerator-Plugins-Config>
</OpenIG-Config>